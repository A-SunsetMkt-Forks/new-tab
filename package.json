{
  "name": "chrome-new-tab",
  "version": "0.0.0",
  "description": "A minimal and high performance Google Chrome new tab page (NTP) extension.",
  "keywords": [
    "chrome-extension",
    "chrome",
    "chrome-app",
    "marko",
    "markojs",
    "css-grid"
  ],
  "author": "Max Milton <max@wearegenki.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "__dev": "NODE_ENV=development browser-refresh build.js",
    "dev": "NODE_ENV=development node build.js",
    "prebuild": "rm -rf ./cache ./dist",
    "build": "NODE_ENV=production node build.js",
    "make": "npm run build && http-server -o ./dist"
  },
  "dependencies": {
    "marko": "^4.7.5",
    "marko-bind": "^0.1.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.58",
    "@types/lasso": "^2.4.0",
    "@wearegenki/eslint-config": "0.4.0-alpha.09cfe267",
    "@wearegenki/stylelint-config": "0.4.0-alpha.09cfe267",
    "eslint": "^4.16.0",
    "lasso": "^3.1.2",
    "lasso-marko": "^2.4.0",
    "stylelint": "^8.4.0"
  },
  "engines": {
    "node": ">= 9.4.0",
    "npm": ">= 3.10.10"
  },
  "browserslist": [
    "last 2 Chrome major versions"
  ]
}
