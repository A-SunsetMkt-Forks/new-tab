{
  "name": "new-tab",
  "version": "0.3.1",
  "description": "A high performance new tab page that gets you where you need to go faster.",
  "keywords": [
    "google-chrome",
    "ntp",
    "chrome-extension",
    "chrome",
    "marko",
    "markojs",
    "lasso"
  ],
  "author": "Max Milton <max@wearegenki.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development node build/watch.js",
    "prewatch": "npm run prep",
    "watch": "NODE_ENV=production node build/watch.js",
    "prep": "rm -rf .cache dist && mkdir -p dist/src",
    "prebuild": "npm run prep",
    "build": "APP_RELEASE=$(git describe --always) NODE_ENV=production node build/build.js",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"{src,test}/**/*.{marko,html,css}\"",
    "lint:js": "eslint --ext '.js,.marko' src/ test/ build/",
    "test": "echo 'TODO: Test suite and tests' && exit 1"
  },
  "dependencies": {
    "raven-js": "^3.22.2"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.59",
    "@wearegenki/eslint-config": "^0.4.2",
    "@wearegenki/postcss-config": "^0.4.2",
    "@wearegenki/stylelint-config": "^0.4.2",
    "browser-sync": "^2.23.6",
    "clean-css": "^4.1.9",
    "eslint": "^4.17.0",
    "lasso": "^3.1.2",
    "lasso-marko": "^2.4.0",
    "lasso-postcss": "^0.4.0",
    "marko": "^4.7.5",
    "marko-bind": "^0.1.0",
    "optimize-js": "^1.0.3",
    "postcss": "^6.0.17",
    "stylelint": "^8.4.0",
    "uglify-es": "^3.3.10"
  },
  "engines": {
    "node": ">= 6.9.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
