{
  "name": "new-tab",
  "version": "0.5.0",
  "description": "A high performance new tab page that gets you where you need to go faster.",
  "keywords": [
    "google-chrome",
    "ntp",
    "chrome-extension",
    "chrome",
    "marko",
    "markojs",
    "lasso"
  ],
  "author": "Max Milton <max@wearegenki.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development node build/watch.js",
    "prewatch": "npm run prep",
    "watch": "NODE_ENV=production node build/watch.js",
    "prep": "rm -rf .cache dist && mkdir -p dist/src",
    "prebuild": "npm run prep",
    "build": "APP_RELEASE=$(git describe --always --dirty=\"-dev\") NODE_ENV=production node build/build.js",
    "lint:css": "stylelint \"{src,test}/**/*.{marko,html,css}\"",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.marko src test",
    "lint": "npm run lint:css && npm run lint:js",
    "pretest": "npm run lint",
    "test": "jest --config test/jest.config.js --coverage",
    "test-ci": "yarn test --runInBand",
    "test:watch": "yarn test --notify --watch"
  },
  "dependencies": {
    "raven-js": "3.23.1"
  },
  "devDependencies": {
    "@types/chrome": "0.0.60",
    "@types/jest": "22.2.0",
    "@wearegenki/eslint-config": "0.5.0-alpha.f1eec7f5",
    "@wearegenki/postcss-config": "0.5.0-alpha.9dd6da21",
    "@wearegenki/stylelint-config": "0.5.0-alpha.9dd6da21",
    "@wearegenki/test": "0.5.0-nightly.8dc8f30",
    "browser-sync": "2.23.6",
    "clean-css": "4.1.11",
    "eslint": "4.18.2",
    "jest": "22.4.2",
    "lasso": "3.1.2",
    "lasso-marko": "2.4.0",
    "lasso-postcss": "0.4.0",
    "marko": "4.9.1",
    "marko-bind": "0.1.0",
    "optimize-js": "1.0.3",
    "postcss": "6.0.19",
    "puppeteer": "1.1.1",
    "stylelint": "9.1.1",
    "uglify-es": "3.3.10",
    "worker-nodes": "1.6.0"
  },
  "engines": {
    "node": ">= 6.9.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
